    }
  },
  "categories": [
    "Desktop"
  ],
  "friendly_name": "Xpipe",
  "architecture": [
    "amd64",
    "arm64"
  ],
  "compatibility": [
    {
      "version": "1.16.x",
      "version": "1.17.x",
      "image": "ghcr.io/xpipe-io/xpipe-webtop:latest",
      "available_tags": [
        "Desktop"
      ]
    }
  ]
}
